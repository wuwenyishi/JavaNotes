<script>
    window.addEventListener('TelegramBotPinned', function(event) {
        $('.pbot-name').attr('href', '@' + event.detail.name);
        $('.pbot-name span').html('@' + event.detail.name);
    });
    $(document).ready(function() {
        let extentionsDecor = {
            timeout: null,
            nodes: $('.extensions_decor'),
            pointer: 0,
            start: function() {
                $(this.nodes).hide();
                $(this.nodes[this.pointer]).show();
                if (this.pointer < 2) {
                    this.pointer++;
                } else {
                    this.pointer = 0;
                }
                this.timeout = setTimeout(() => {
                    this.start()
                }, 3000);
            },
            stop: function() {
                clearTimeout(this.timeout);
                this.pointer = 0;
            }
        }


        $('.page-title-back-arrow').attr('href', document.referrer)
        $('.pbot-name').attr('href', '@' + window.TelegramBotName);
        $('.pbot-name span').html('@' + window.TelegramBotName);
        $('.js-tab').on('click', function() {
            const id = $(this).attr('id');
            location.hash = id + '_tab';
            $('.js-tab').removeClass('_active');
            $(this).addClass('_active');

            $('*[data-type]').removeClass('_active');
            $('*[data-type="' + id + '"]').addClass('_active');
            if (id === 'browser_extensions') {
                extentionsDecor.start();
            } else {
                extentionsDecor.stop();
            }
        })

        $('.js-copy-domain').on('click', function(e) {
            e.preventDefault();
            let link;
            link = $(this).attr('data-href')
            if (!link) {
                link = $($(this).find('a span')[0]).html()
            }
            navigator.clipboard.writeText(link)
            new ZLibraryNotify().info('Address is copied to your clipboard')
        })

        const $tgStatusLoading = $('#tgStatusLoading')
        if ($tgStatusLoading.length) {
            new ZLibrarySpinner($tgStatusLoading).color('#7d7d7d').start()
            doLoadTelegramBindingStatus()
        }

        if (location.hash) {
            $(location.hash.slice(0, -4)).click();
        }
        copy = function(text) {
            navigator.clipboard.writeText(text)
            new ZLibraryNotify().info('Address is copied to your clipboard')
        }
        createAppLoginModal = function() {
            $("#zlibrary-modal-styled").find('.simplebar-content').html('');
            $(".modal-dialog").removeClass("tg-connect-dialog");

            new ZLibraryModal({
                container: 'zlibrary-modal-styled',
                element: 'android-app-modal',
                // title: 'How to connect bot',
                modalDialogClass: 'android-app-dialog',
                width: 348,
                autoFocus: false,
                afterRender: function() {
                    new SimpleBar(($('.android-app-modal-content').parent('.modal-body'))[0], {
                        autoHide: false
                    })
                }
            }).show()
        }
        $(window).on('modalOpened', function(e) {
            if (e.detail.name == 'android-app-modal') {
                $('.za-tabs__android-data-qr').hide()
            }
        })
        $(window).on('modalClosed', function(e) {
            if (e.detail.name == 'android-app-modal') {
                $('.za-tabs__android-data-qr').show()
            }
        })
        $('#zlibrary-modal-styled').on('hidden.bs.modal', function(e) {
            $('.za-tabs__android-data-qr').show()
        })

        checkOSIcon = function() {
            $('.za-tab__os_icon').hide();
            if (window.innerWidth > 768) {
                if (navigator.platform.startsWith('Win')) {
                    $("#os_icon1").show()
                } else if (navigator.platform.startsWith('Linux')) {
                    $("#os_icon3").show()
                } else if (navigator.platform.startsWith('Mac')) {
                    $("#os_icon2").show()
                } else {
                    $("#os_icon1").show()
                }
            } else {
                $("#os_icon4").show()
            }
        }
        checkOSIcon();
        if (window.innerWidth < 769) {
            $("#android_app").click();
        }
        $(window).on('resize', function() {
            checkOSIcon();
        })

        $('.za-socials a').tooltip({
            container: 'body',
            placement: 'bottom',
        })
        var userAgent = navigator.userAgent.toLowerCase();

        if (userAgent.indexOf('chrome') > -1) {
            $("#plugin-not-installed-button").attr('href', '#')
        } else if (userAgent.indexOf('firefox') > -1) {
            $("#plugin-not-installed-button").attr('href', 'https://addons.mozilla.org/en-US/firefox/addon/z-library/')
        }
    })
</script>                                                                    </div>
